URxvt.font: xft:Monaco for Powerline:size=8
URxvt.scrollBar: false

! True transparency
!URxvt.depth: 32
!URxvt.background: [75]#000000

! Fake transparency
URxvt.transparent: true
URxvt.shading: 15

Xft.autohint: 0
Xft.lcdfilter: lcddefault
Xft.hintstyle: hintslight
Xft.hinting: 1
Xft.antialias: 1
Xft.rgba: rgb
URxvt.letterSpace: -2

Xcursor.size: 16
Xcursor.theme: Adwaita

! terminal colours
*foreground:#a0a0a0
*background:#000000

!molokai inspired colors

!black
*color0:    #1b1d1e
*color8:    #505354
!red
*color1:    #f92672
*color9:    #ff5995
!green
*color2:    #82b414
*color10:   #b6e354
!yellow
*color3:    #fd971f
*color11:   #feed6c
!blue
*color4:    #56c2d6
*color12:   #8cedff
!magenta
*color5:    #8c54fe
*color13:   #9e6ffe
!cyan
*color6:    #465457
*color14:   #899ca1
!white
*color7:    #ccccc6
*color15:   #f8f8f2
